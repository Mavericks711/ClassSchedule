/* 基础样式：移除navigator默认点击效果 */
navigator {
  -webkit-tap-highlight-color: transparent; /* 清除iOS点击高亮 */
  display: inline;
  position: relative;
  overflow: hidden;
}

navigator::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: transparent; /* 点击区域透明 */
  pointer-events: none; /* 不影响点击事件 */
}

/* 注册文字原始样式 */
.text_2 {
  color: #a6a6a6; /* 浅灰色文字 */
  font-weight: 700;
  letter-spacing: 4rpx;
}

/* 注册文字点击动画 */
.registration-animation {
  transition: all 0.3s ease; /* 过渡动画 */
}

.registration-animation:active {
  transform: scale(1.5); /* 放大% */
  color: #000000; /* 变为黑色 */
}

/* --- 6. 提交按钮 --- */
.text-wrapper {
  padding: 0;
  border: none;
  /* 【解决】增大按钮和上方元素的呼吸感 */
  margin-top: 20rpx; 
  width: 100% !important;
  
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  border-radius: 48rpx;
  background-color: white;
  border: solid 2rpx #1e8aef;
  color: #1e8aef;
  font-size: 34rpx;
  font-weight: 700;
  letter-spacing: 10rpx; /* 增加文字间距，提升设计感 */
  
}


button.text-wrapper::after {
  display: none;
}

/* 按钮按下的效果 */
button.text-wrapper:active {
  background-color: #f0f0f0;
}